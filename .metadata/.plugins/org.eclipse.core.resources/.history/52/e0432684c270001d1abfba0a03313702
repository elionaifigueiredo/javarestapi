package com.algaworks.apirest.listener;

import org.springframework.stereotype.Component;

import com.algaworks.apirest.service.ClienteAtivadoEvent;


@Component
public class EmissaoNotaFiscalService {
	
	
	
	public void clienteAtivadoListerner(ClienteAtivadoEvent event) {
		System.out.println("Emitindo a nota fiscal para cliente " + event.getCliente().getNome());
	}
	
	

}
